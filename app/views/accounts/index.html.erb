<h1>Accounts#index</h1>
<p>Find me in app/views/accounts/index.html.erb</p>
<h2>Bank</h2>
<% @bank.each do |bank| %>
<div class="panel panel-primary">
<%= bank.name %>
<%= bank.balance %>
</div>
<% end %>


<h2>Cash</h2>
<div class="panel panel-primary">
<% @cash.each do |cash| %>
<%= cash.name %>
<%= cash.balance %>
</div>
<% end %>


<h2>Credit Cards</h2>
<div class="panel panel-primary">
<% @credit.each do |credit| %>
<%= credit.name %>
<%= credit.balance %>
<%= credit.limit %>
</div>
<% end %>